{
    "name": "friendsofvictoire/form-widget",
    "type": "symfony-bundle",
    "description": "Create custom forms in Victoire website",
    "keywords": [
        "DCMS",
        "Victoire",
        "widget",
        "FriendsofVictoire",
        "Form"
    ],
    "license": "MIT",
    "authors": [
        {
            "name" : "Leny BERNARD",
            "email": "leny@victoire.io"
        },
        {
            "name" : "Charlie Lucas",
            "email": "charlie@victoire.io"
        }
    ],
    "require"          : {
        "php"                      : ">=5.3.3",
        "symfony/framework-bundle" : "~2.3",
        "victoire/victoire"        : "~1.3.0",
        "appventus/alertify-bundle": "^1.0",
        "ornicar/akismet-bundle"   : "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Victoire\\Widget\\FormBundle": ""
        }
    },
    "minimum-stability": "dev",
    "target-dir": "Victoire/Widget/FormBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}
